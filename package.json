{
  "name": "web-audio-synthesizer",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.16",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "node-sass": "^7.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "sass-loader": "^13.2.1",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "customize-cra": "^1.0.0",
    "react-app-rewired": "^2.2.1"
  },
  "jest": {
    "moduleNameMapper": {
      "^@components(/.*)$": "<rootDir>/src/components$1",
      "^@parts(/.*)$": "<rootDir>/src/components/parts$1",
      "^@circuits(/.*)$": "<rootDir>/src/components/circuits$1",
      "^@controls(/.*)$": "<rootDir>/src/components/controls$1",
      "^@utils(/.*)$": "<rootDir>/src/utils$1",
      "^@styles(/.*)$": "<rootDir>/src/styles$1",
      "^@assets(/.*)$": "<rootDir>/src/assets$1",
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "roots": [
      "<rootDir>/tests"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/tests/**/*.test.(tsx|ts)"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ]
  }
}
